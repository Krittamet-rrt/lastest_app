generator client {
  provider   = "dart run orm"
  output     = "../lib/_generated_prisma_client"
  engineType = "flutter"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

// create model for this project
model LastestTimeItem {
  id        Int       @id @default(autoincrement())
  name      String
  isChecked Boolean   @default(false)
  cycleExp  Int
  markTime  DateTime?
  isPinned  Boolean   @default(false)
}
